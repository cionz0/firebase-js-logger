{
  "name": "@cionzo/firebase-js-logger",
  "version": "2.0.0",
  "description": "A Winston-based logger with automatic file and line number tracking for Node.js applications",
  "main": "src/index.js",
  "scripts": {
    "test": "node test/index.test.js",
    "test:integration": "node test/integration.test.js",
    "test:all": "npm run test && npm run test:integration",
    "bump:patch": "verbump-js patch --generate-changelog",
    "bump:minor": "verbump-js minor --generate-changelog",
    "bump:major": "verbump-js major --generate-changelog",
    "bump:patch:push": "verbump-js patch --generate-changelog --push",
    "bump:minor:push": "verbump-js minor --generate-changelog --push",
    "bump:major:push": "verbump-js major --generate-changelog --push",
    "changelog": "verbump-js changelog --generate-changelog"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cionz0/firebase-js-logger.git"
  },
  "keywords": [
    "logger",
    "logging",
    "winston",
    "console",
    "log",
    "debug",
    "debugging",
    "firebase",
    "nodejs",
    "node",
    "stack-trace",
    "line-number",
    "file-tracking",
    "source-tracking",
    "error-logging",
    "info",
    "warn",
    "error",
    "timestamp",
    "formatted-logging",
    "cloud-logging",
    "google-cloud"
  ],
  "author": "cionzo <cionzoh@gmail.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/cionz0/firebase-js-logger/issues"
  },
  "homepage": "https://github.com/cionz0/firebase-js-logger#readme",
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@cionz0/verbump-js": "github:cionz0/verbump-js"
  }
}
